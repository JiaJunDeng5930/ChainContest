{
  "name": "chaincontest",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "lint": "pnpm -r --filter '!@chaincontest/dev-console' lint",
    "test": "pnpm -r test",
    "build": "pnpm -r build",
    "db:test": "pnpm --filter @chaincontest/db test",
    "dev:api-server": "pnpm --filter @chaincontest/api-server dev",
    "build:api-server": "pnpm --filter @chaincontest/api-server build",
    "start:api-server": "pnpm --filter @chaincontest/api-server start",
    "test:api-server": "pnpm --filter @chaincontest/api-server test",
    "dev:indexer-event": "pnpm --filter @chaincontest/indexer-event dev",
    "build:indexer-event": "pnpm --filter @chaincontest/indexer-event build",
    "start:indexer-event": "pnpm --filter @chaincontest/indexer-event start",
    "test:indexer-event": "pnpm --filter @chaincontest/indexer-event test",
    "lint:indexer-event": "pnpm --filter @chaincontest/indexer-event lint"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2"
  }
}
